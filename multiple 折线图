#基本图表
#折线图
plot(dt$consumption~dt$Time,pch=15,col="Black",ylim=c(-50,200))
points(dt$Time,dt$x1,pch=16,col="DeepPink",cex=1)
points(dt$Time,dt$x2,pch=17,col="RosyBrown",cex=1)
points(dt$Time,dt$x3,pch=18,col="DarkTurquoise",cex=1)
lines(dt$consumption,col="Black",lty=1)
lines(dt$x1,col="DeepPink",lty=2)
lines(dt$x2,col="RosyBrown",lty=3)
lines(dt$x3,col="DarkTurquoise",lty=4)

